      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "react": "^19.2.0"
  },
  "devDependencies": {
    "@types/react": "19.2.2",
    "esbuild": "^0.27.0",
    "esbuild-node-externals": "^1.20.1",
    "typescript": "^5.9.3"
  }
}

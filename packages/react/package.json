{
  "name": "@mooduck/react",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "build": "node esbuild.config.js && tsc --emitDeclarationOnly",
    "typecheck": "tsc --noEmit"
